  "authors": [
    {
      "name": "Michael",
      "email": "chrismichaels84@gmail.com",
      "homepage": "http://chrismichaelsauthor.com",
      "role": "Lead Developer"
    }
  ],
  "require": {
    "php" : ">=5.6.0",
